program myRLike;
let datay: int[30];
let datax: int[15];
let arraySize: int;


function fillArrayData: void()
let i: int;
{
  for(i = 0 to i == arraySize by 1) {
    datay[i] = random(25, 30);
    print(datay[i], " ");
  }
  println("");
}

function fillArrayData2: void()
let i: int;
{
  for(i = 0 to i == 15 by 1) {
    datax[i] = random(1, 5);
    print(datax[i], " ");
  }
  println("");
}

main()
{
  arraySize = 30;
  println("Los dos arreglos de datos:");
  fillArrayData();
  fillArrayData2();
  
  plot(datay, datax);

}






