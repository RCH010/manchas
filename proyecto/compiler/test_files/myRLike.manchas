  }
}

function printData: void()
let i: int;
{
  println("");
  for(i = 0 to i == arraySize by 1) {
    print(data[i], " ");
  }
  println("");
}

main()
{
  arraySize = 20;
  println("Las edades de un grupo de 20 personas es:");
